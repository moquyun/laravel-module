{
    "name": "moquyun/laravel-module",
    "description": "laravel modules",
    "license": "MIT",
    "authors": [
        {
            "name": "moquyun",
            "email": "pp@moquyun.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "\\Moquyun\\Module\\LaravelServiceProvider"
            ],
            "aliases": {
                "HDModule": "\\Moquyun\\Module\\Factory"
            }
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "nwidart/laravel-modules": "^4.0",
        "spatie/laravel-permission": "^2.12",
        "houdunwang/laravel-upload": "^1.0",
        "doctrine/dbal": "^2.7",
        "houdunwang/laravel-autocreate": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Moquyun\\Module\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moquyun\\Module\\Tests\\": "tests/"
        }
    }
}
